{
  "name": "fastify-vitest-app",
  "version": "1.0.0",
  "description": "A basic Fastify application with Vitest tests",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": ["fastify", "vitest", "api", "typescript"],
  "license": "MIT",
  "dependencies": {
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2",
    "vitest": "^3.0.4",
    "@vitest/coverage-v8": "^3.0.4"
  }
}
